{
  "version": 2,
  "name": "line-shop-diagnosis-bot",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/index.js"
    }
  ],
  "env": {
    "CHANNEL_ACCESS_TOKEN": "@line-channel-access-token",
    "CHANNEL_SECRET": "@line-channel-secret"
  }
}